{
  "name": "coconut-analytics",
  "version": "0.9.0",
  "main": "app/start.coffee",
  "scripts": {
    "start": "npm run watch & npm run livereload & npm run static",
    "watch": "./node_modules/watchify/bin/cmd.js -v -t coffeeify --debug --extension='.coffee' app/start.coffee -x moment -x jquery -x backbone -x pouchdb-core -x pouchdb-adapter-http -x pouchdb-mapreduce -x pouchdb-upsert -x underscore -x tabulator-tables  -o bundle.js",
    "livereload": "./node_modules/livereloadify/cli.js ./",
    "static": "./node_modules/node-static/bin/cli.js --host-address 0.0.0.0 --port 8082 ./",
    "install-server": "npm install --no-optional --unsafe-perm",
    "postinstall": "pwd",
    "bundlify": "npx browserify -v -t coffeeify --extension='.coffee' app/start.coffee -x moment -x jquery -x backbone -x pouchdb-core -x pouchdb-adapter-http -x pouchdb-mapreduce -x pouchdb-upsert -x underscore -x tabulator-tables > bundle.js",
    "vendorify": "npx browserify -r moment -r jquery -r backbone -r pouchdb-core -r pouchdb-adapter-http -r pouchdb-mapreduce -r pouchdb-upsert -r underscore -r tabulator-tables > vendor.min.js"
  },
  "dependencies": {
    "backbone": "^1.2.1",
    "backbone-pouch": "^1.5.0",
    "Base64": "^0.3.0",
    "bootstrap-daterangepicker": "^2.1.24",
    "chart.js": "^2.8.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "choices.js": "^9.0.1",
    "copy-text-to-clipboard": "^2.1.0",
    "crossfilter": "^1.3.12",
    "d3": "^3.5.17",
    "d3-scale": "^1.0.3",
    "datatables.net": "^1.10.12",
    "datatables.net-dt": "^1.10.12",
    "date-fns": "^2.16.1",
    "dc": "^2.1.7",
    "dialog-polyfill": "^0.4.7",
    "distinct-colors": "^3.0.0",
    "file-saver": "^1.3.8",
    "form2js": "^1.0.0",
    "fuse.js": "^6.4.0",
    "geojson-geometries-lookup": "^0.5.0",
    "html2pdf.js": "^0.9.1",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-browserify": "^1.11.0-pre-seelio",
    "js-cookie": "^2.1.1",
    "leaflet": "^1.5.1",
    "leaflet-image": "^0.1.0",
    "leaflet-material-controls": "^1.0.0",
    "leaflet.heat": "^0.2.0",
    "mapbox.js": "^3.2.0",
    "material-design-icons": "^2.0.0",
    "material-design-lite": "^1.1.3",
    "materialize": "^1.0.0",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-range": "^2.2.0",
    "pivottable": "^2.23.0",
    "polylabel": "^1.0.2",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-browser": "^7.1.1",
    "pouchdb-core": "^7.2.2",
    "pouchdb-mapreduce": "^7.2.2",
    "pouchdb-security": "^4.2.0",
    "pouchdb-security-helper": "^2.1.2",
    "pouchdb-upsert": "^2.2.0",
    "radix-64": "^1.0.4",
    "react": "^0.14.8",
    "sha1": "^1.1.1",
    "string-strip-html": "^4.3.16",
    "tablesorter": "^2.25.4",
    "tabulator-tables": "^4.7.0",
    "time-stamp": "^2.2.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "chance": "^1.0.4",
    "coffee": "^4.1.0",
    "coffeeify": "^3.0.1",
    "coffeescript": "^2.4.1",
    "forever": "^0.15.2",
    "livereloadify": "^2.0.0",
    "node-static": "*",
    "uglify-js": "^3.4.9",
    "watchify": "^3.6.1"
  }
}
